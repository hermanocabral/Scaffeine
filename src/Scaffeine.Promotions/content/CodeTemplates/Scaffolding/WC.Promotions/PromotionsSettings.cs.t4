            get { return (ProcessorSectionSettingsCollection)this["prerequisites"]; }
        }

        [ConfigurationProperty("benefits")]
        public ProcessorSectionSettingsCollection Benefits
        {
            get { return (ProcessorSectionSettingsCollection)this["benefits"]; }
        }
    }
}
