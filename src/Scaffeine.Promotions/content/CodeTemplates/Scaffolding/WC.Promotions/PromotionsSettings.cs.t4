using System.Configuration;
using MvcApplication3.Pipeline;

namespace Promotions
{
    public partial class PromotionsSettings : ConfigurationElement
    {
        [ConfigurationProperty("code")]
        public string Code { get { return this["code"] as string; } }

        [ConfigurationProperty("prerequisites")]
        public ProcessorSectionSettingsCollection Prerequisites
